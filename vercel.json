{
  "rewrites": [
    {
      "source": "/heroImage/:path*",
      "destination": "https://image.tmdb.org/t/p/w1280/:path*"
    },
    {
      "source": "/downloadImage/:path*",
      "destination": "https://image.tmdb.org/t/p/original/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
